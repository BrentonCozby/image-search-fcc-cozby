<div class="row">
    <form class="form col s12">
        <div class="row">
            <div class="form-search input-field col s6">
                <input name="search" id="search" type="text" class="validate" required>
                <label for="search">Search Term&nbsp;&nbsp;|&nbsp;&nbsp;search</label>
            </div>
        </div>
        <div class="row">
            <h6 class="form-option-title col s12">How Recent?&nbsp;&nbsp;|&nbsp;&nbsp;dateRestrict</h6>
            <div class="form-dateRestrict input-field col s12">
                <div class="col s1">
                    <input name="dateRestrictNum" type="text" placeholder="num" min="1" max="" id="date-restrict-num">
                </div>
                <div class="col s2">
                    <select name="dateRestrictType">
                        <option value="" disabled selected>Period</option>
                        <option value="y">Years</option>
                        <option value="m">Months</option>
                        <option value="w">Weeks</option>
                        <option value="d">Days</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <h6 class="form-option-title col s12">Image Size&nbsp;&nbsp;|&nbsp;&nbsp;imgSize</h6>
            <div class="form-imgSize input-field form-radio-field col s12">
                <input class="form-radio" name="imgSize" value="huge" type="radio" id="huge">
                <label class="waves-effect waves-teal btn" for="huge">huge</label>

                <input class="form-radio" name="imgSize" value="xxlarge" type="radio" id="xxlarge">
                <label class="waves-effect waves-teal btn" for="xxlarge">xxlarge</label>

                <input class="form-radio" name="imgSize" value="xlarge" type="radio" id="xlarge">
                <label class="waves-effect waves-teal btn" for="xlarge">xlarge</label>

                <input class="form-radio" name="imgSize" value="large" type="radio" id="large">
                <label class="waves-effect waves-teal btn" for="large">large</label>

                <input class="form-radio" name="imgSize" value="medium" type="radio" id="medium">
                <label class="waves-effect waves-teal btn" for="medium">medium</label>

                <input class="form-radio" name="imgSize" value="small" type="radio" id="small">
                <label class="waves-effect waves-teal btn" for="small">small</label>

                <input class="form-radio" name="imgSize" value="icon" type="radio" id="icon">
                <label class="waves-effect waves-teal btn" for="icon">icon</label>
            </div>
        </div>
        <div class="row">
            <h6 class="form-option-title col s12">Dominant Color&nbsp;&nbsp;|&nbsp;&nbsp;imgDominantColor</h6>
            <div class="form-imgDominantColor input-field col s12">
                <input class="form-radio" name="imgDominantColor" value="black" type="radio" id="black">
                <label class="waves-effect waves-teal btn" for="black">black</label>

                <input class="form-radio" name="imgDominantColor" value="white" type="radio" id="white">
                <label class="waves-effect waves-teal btn" for="white">white</label>

                <input class="form-radio" name="imgDominantColor" value="gray" type="radio" id="gray">
                <label class="waves-effect waves-teal btn" for="gray">gray</label>

                <input class="form-radio" name="imgDominantColor" value="brown" type="radio" id="brown">
                <label class="waves-effect waves-teal btn" for="brown">brown</label>

                <input class="form-radio" name="imgDominantColor" value="blue" type="radio" id="blue">
                <label class="waves-effect waves-teal btn" for="blue">blue</label>

                <input class="form-radio" name="imgDominantColor" value="green" type="radio" id="green">
                <label class="waves-effect waves-teal btn" for="green">green</label>

                <input class="form-radio" name="imgDominantColor" value="teal" type="radio" id="teal">
                <label class="waves-effect waves-teal btn" for="teal">teal</label>

                <input class="form-radio" name="imgDominantColor" value="yellow" type="radio" id="yellow">
                <label class="waves-effect waves-teal btn" for="yellow">yellow</label>

                <input class="form-radio" name="imgDominantColor" value="pink" type="radio" id="pink">
                <label class="waves-effect waves-teal btn" for="pink">pink</label>

                <input class="form-radio" name="imgDominantColor" value="purple" type="radio" id="purple">
                <label class="waves-effect waves-teal btn" for="purple">purple</label>
            </div>
        </div>
        <div class="row">
            <h6 class="form-option-title col s12">Color Type&nbsp;&nbsp;|&nbsp;&nbsp;imgColorType</h6>
            <div class="form-imageColorType input-field col s12">
                <input class="form-radio" name="imgColorType" value="color" type="radio" id="color">
                <label class="waves-effect waves-teal btn" for="color">color</label>

                <input class="form-radio" name="imgColorType" value="gray" type="radio" id="gray">
                <label class="waves-effect waves-teal btn" for="gray">gray</label>

                <input class="form-radio" name="imgColorType" value="mono" type="radio" id="mono">
                <label class="waves-effect waves-teal btn" for="mono">mono</label>
            </div>
        </div>
    </form>
</div>
